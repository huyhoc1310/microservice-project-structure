[default]
SQLALCHEMY_TRACK_MODIFICATIONS=false
FULL_DATETIME='%Y-%m-%d %H:%M:%S'

[development]
DEBUG=true
SQLALCHEMY_DATABASE_URI='mysql://<username>:<password>@<host>:<port>/<database-name>'
SQLALCHEMY_TRACK_MODIFICATIONS=true

[production]
DEBUG=false
SQLALCHEMY_DATABASE_URI='mysql://<username>:<password>@<host>:<port>/<database-name>'

[testing]
DEBUG = true
SQLALCHEMY_DATABASE_URI = 'sqlite:///'
TESTING = true
